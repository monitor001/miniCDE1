{
  "root": "build/",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31557600"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "error_page": "index.html"
} 